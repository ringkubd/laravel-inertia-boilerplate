{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "require": {
        "php": "^8.0",
        "ext-openssl": "*",
        "ext-zip": "*",
        "alimranahmed/laraocr": "^1.2",
        "anwar/auto-login": "dev-main",
        "anwar/config-editor": "dev-main",
        "awobaz/compoships": "^2.1",
        "barryvdh/laravel-dompdf": "^0.9.0",
        "beyondcode/laravel-websockets": "^1.13",
        "ckfinder/ckfinder-laravel-package": "v3.5.2.1",
        "doctrine/dbal": "^3.1",
        "fideloper/proxy": "^4.4",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "haruncpi/laravel-simple-filemanager": "^1.0",
        "http-interop/http-factory-guzzle": "^1.2",
        "inertiajs/inertia-laravel": "^0.4.1",
        "intervention/image": "^2.7",
        "laravel/framework": "^8.40",
        "laravel/sanctum": "^2.11",
        "laravel/scout": "^9.3",
        "laravel/tinker": "^2.5",
        "league/fractal": "^0.19.2",
        "maatwebsite/excel": "^3.1",
        "meilisearch/meilisearch-php": "^0.23.1",
        "predis/predis": "^2.0",
        "relative/laravel-expo-push-notifications": "^1.0",
        "simplesoftwareio/simple-qrcode": "~4",
        "spatie/laravel-permission": "^4.2",
        "svnwa/inertiavuepaginator": "^1.0",
        "tightenco/ziggy": "^1.0",
        "webklex/laravel-imap": "^2.4"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.6",
        "facade/ignition": "^2.5",
        "fakerphp/faker": "^1.9.1",
        "laravel/breeze": "^1.3",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.2",
        "nunomaduro/collision": "^5.0",
        "phpunit/phpunit": "^9.3.3"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "app/Helpers/conversation.php",
            "app/Helpers/settings.php",
            "app/Helpers/common.php",
            "app/Helpers/pusher.php",
            "app/Helpers/Api/Constants.php",
            "app/Helpers/Api/ResponseHelper.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "path",
            "url": "./packages/*"
        }
    ]
}
