{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "require": {
        "php": "^8.0",
        "awobaz/compoships": "^2.1",
        "beyondcode/laravel-websockets": "^1.12",
        "ckfinder/ckfinder-laravel-package": "v3.5.2.1",
        "doctrine/dbal": "^3.1",
        "fideloper/proxy": "^4.4",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "haruncpi/laravel-simple-filemanager": "^1.0",
        "http-interop/http-factory-guzzle": "^1.2",
        "inertiajs/inertia-laravel": "^0.4.1",
        "intervention/image": "^2.6",
        "laravel/framework": "^8.40",
        "laravel/sanctum": "^2.11",
        "laravel/scout": "^9.3",
        "laravel/tinker": "^2.5",
        "league/fractal": "^0.19.2",
        "maatwebsite/excel": "^3.1",
        "meilisearch/meilisearch-php": "^0.20.0",
        "spatie/laravel-permission": "^4.2",
        "svnwa/inertiavuepaginator": "^1.0",
        "tightenco/ziggy": "^1.0"
    },
    "require-dev": {
        "facade/ignition": "^2.5",
        "fakerphp/faker": "^1.9.1",
        "laravel/breeze": "^1.3",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.2",
        "nunomaduro/collision": "^5.0",
        "phpunit/phpunit": "^9.3.3"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "app/Helpers/conversation.php",
            "app/Helpers/settings.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
